cfa_fit <- list()
cfa_fit_data <- list()
cfa_fit_data_merge <- list()
## Extract unique time and sex values
timepoints <- unique(x_var_data_soc$VNUM_2)
sex_vals <- unique(x_var_data_soc$GCHAR)
for(i in 1:length(timepoints)){
cfa_fit[[i]] <- list()
cfa_fit_data[[i]] <- list()
## Define FA model based on items at given time point
if(timepoints[i]%in%c("6","12")){
formula_vars_timespecific <-
items_soc[!grepl("QQSO|QSHOW",items_soc)]
}else{
if(timepoints[i]%in%c("24","36")){
formula_vars_timespecific <-
items_soc[!grepl("Q5|Q9",items_soc)]
}else{
formula_vars_timespecific <- items_soc
}
}
for(j in 1:length(sex_vals)){
## Extract only data for current time, sex
x_var_data_soc_subset <- x_var_data_soc %>% filter(VNUM_2==timepoints[i]&GCHAR==sex_vals[j])
## Create CFA model spec
efa_1fact_model_obj <-
paste0('#factor loadings \n ',
"soccfa","=~ ",paste0(formula_vars_timespecific,
collapse = " + "),
"\n #covariances")
## Fit CFA, extract scores
cfa_fit[[i]][[j]] <- cfa(efa_1fact_model_obj, data=x_var_data_soc_subset, meanstructure=TRUE,
missing="fiml")
cfa_fit_data[[i]][[j]] <- cbind(x_var_data_soc_subset, lavPredict(cfa_fit[[i]][[j]]))
}
## Bind all scores for given time point
cfa_fit_data_merge[[i]] <- do.call("rbind", cfa_fit_data[[i]])
}
cfa_fit_data_merge_final <- do.call("rbind", cfa_fit_data_merge) %>% arrange(ID, VNUM)
